###  http://example.org/biointegrate/Case
bio:Case rdf:type owl:Class ;
         rdfs:subClassOf bio:BioEntity ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty bio:hasCaseMeasurement ;
                           owl:allValuesFrom bio:ExpressionMeasurement
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty bio:caseInProject ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass bio:Project
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty bio:caseId ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty bio:diseaseTypeLabel ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty bio:primarySiteLabel ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string
                         ] ;
         owl:hasKey ( bio:caseId
                    ) ;
         bio:manchesterDefinition "Case SubClassOf (caseId exactly 1 xsd:string) and (caseInProject exactly 1 Project)"@en ;
         dc:source "GDC/TCGA" ;
         rdfs:label "Case"@en .


